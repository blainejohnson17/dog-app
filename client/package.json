{
  "name": "foo-ui",
  "version": "0.0.1",
  "description": "Foo UI",
  "main": "index.js",
  "scripts": {
    "start:dev": "webpack-dev-server --history-api-fallback --https --config ./webpack/webpack.dev.js --hot",
    "start:test": "webpack-dev-server --history-api-fallback --https --config ./webpack/webpack.test.js",
    "start:stage": "webpack-dev-server --history-api-fallback --https --config ./webpack/webpack.stage.js",
    "start:prod": "webpack-dev-server --history-api-fallback --https --config ./webpack/webpack.prod.js",
    "build:dev": "rm -rf dist && webpack --config ./webpack/webpack.dev.js",
    "build:test": "rm -rf dist && webpack --config ./webpack/webpack.test.js",
    "build:stage": "rm -rf dist && webpack --config ./webpack/webpack.stage.js",
    "build:prod": "rm -rf dist && webpack --config ./webpack/webpack.prod.js",
    "test": "node ./node_modules/.bin/jest --coverage",
    "test:watch": "node ./node_modules/.bin/jest --coverage --watchAll",
    "lint": "node ./node_modules/.bin/eslint \"**/*.{js,jsx}\"",
    "lint:fix": "node ./node_modules/.bin/eslint --fix \"**/*.{js,jsx}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "./node_modules/.bin/eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "dependencies": {
    "axios": "^0.19.0",
    "bootstrap": "^4.6.0",
    "classnames": "^2.2.6",
    "core-js": "^3.4.2",
    "dedupe": "^3.0.1",
    "deepmerge": "^3.2.1",
    "jstimezonedetect": "^1.0.6",
    "lodash": "^4.17.11",
    "lodash.noop": "^3.0.1",
    "re-reselect": "^3.2.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^6.1.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-async-to-generator": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@types/jest": "^24.0.15",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "css-loader": "^3.0.0",
    "dotenv": "^8.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "es6-promise": "^4.2.8",
    "eslint": "^5.16.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "file-loader": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.4.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.8.0",
    "jest-cli": "^24.8.0",
    "lint-staged": "^8.2.0",
    "lodash.pickby": "^4.6.0",
    "node-sass": "^4.12.0",
    "prop-types": "^15.7.2",
    "react-svg-loader": "^3.0.3",
    "redux-mock-store": "^1.5.3",
    "resolve-url-loader": "^3.1.0",
